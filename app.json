{
  "expo": {
    "displayName": "AI Recognizer",
    "name": "AI Recognizer",
    "slug": "ai-recognizer",
    "version": "1.0.0",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "airecognizer",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.airecognizer",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#fefffe"
      },
      "package": "com.airecognizer"
    },
    "owner": "vikash129",
    "plugins": [
      "expo-asset",
      "expo-font",
      "expo-web-browser"
    ]
   
  }
}
